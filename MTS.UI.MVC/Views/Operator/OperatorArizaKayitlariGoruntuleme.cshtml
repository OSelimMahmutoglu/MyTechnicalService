
@{
    ViewBag.Title = "OperatorArizaKayitlariGoruntuleme";
    Layout = "~/Views/_LayoutPage.cshtml";
}

<h2>Viewing Operator Saved faults</h2>

<div>
    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-md-8 col-sm-8 col-sm-8">
            <table class="table table-hover">
                <thead>
                    <tr>
                        <th>Saved Fault No</th>
                        <th>Saved Fault Date</th>
                        <th>Location</th>
                        <th>Description</th>
                        <th>Is Fixed?</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <th>@item.ArizaKayitNumarasi</th>
                            <td>@item.ArizaKayitZamani</td>
                            <td>@item.Konum</td>
                            <td>@item.Aciklama</td>
                            <td>@item.ArizaGiderildiMi</td>
                        </tr>
                    }

                </tbody>
            </table>

        </div>
    </div>
</div>